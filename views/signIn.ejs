<link rel="stylesheet" href="<%=obj.cssPath%>" />
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>

<div class="form-container" id="contain">
<div class="container" id="container">
	<div class="form-container sign-up-container">
		<div id="form">
			<h1>Create Account</h1>
			
			<span>or use your email for registration</span>
			<input type="text" placeholder="UserName" />
			<input type="password" placeholder="Password" />
			<button id="signup" onclick="userValidation('signUp')" class="submitButton">Sign Up</button>
		</div>
	</div>
	<div class="form-container sign-in-container">
		<div id="form">
			<h1>Sign in</h1>
			<span>or use your account</span>
			<input type="text" placeholder="UserName" />
			<input type="password" placeholder="Password" />
			<button id="signin" onclick="userValidation('signIn')" class="submitButton">Sign In</button>
		</div>
	</div>
	<div class="overlay-container">
		<div class="overlay">
			<div class="overlay-panel overlay-left">
				<h1>Welcome Back!</h1>
				<p>To keep connected with us please login with your personal info</p>
				<button class="ghost" id="signIn">Sign In</button>
			</div>
			<div class="overlay-panel overlay-right">
				<h1>Hello, Friend!</h1>
				<p>Enter your personal details and start journey with us</p>
				<button class="ghost" id="signUp">Sign Up</button>
			</div>
		</div>
	</div>
</div>
  <div id="messageBox"> <div id="mainMessage"> <h1 id="messageText"></h1> <button id="acceptButton" onclick='closeBox()' >OK</button> </div></div>
  </div>



<script src="<%= obj.jsPath %>" >
</script>

